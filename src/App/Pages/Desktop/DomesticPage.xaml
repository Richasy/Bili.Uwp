<local:DomesticPageBase
    x:Class="Bili.App.Pages.Desktop.DomesticPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:behaviors="using:Bili.App.Behaviors"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:interactivity="using:Microsoft.Xaml.Interactivity"
    xmlns:local="using:Bili.App.Pages.Desktop"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:pgc="using:Bili.App.Controls.Pgc"
    mc:Ignorable="d">

    <pgc:AnimeView ViewModel="{x:Bind ViewModel}">
        <interactivity:Interaction.Behaviors>
            <behaviors:InitializeBehavior />
        </interactivity:Interaction.Behaviors>
    </pgc:AnimeView>
</local:DomesticPageBase>
